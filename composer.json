{
    "name": "elixir/elixir-mail",
    "description": "Component mail",
    "version": "1.0.0",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "mail"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-mail",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-view"
        }
    ],
    "require": {
        "php": ">=5.4",
        "swiftmailer/swiftmailer": "~5.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\Mail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\Mail\\": "tests/"
        }
    },
    "suggest": {
        "elixir/elixir-view": "*"
    }
}